<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="help_sove.mapper.Help_soveMapper">

    <insert id="soveit" parameterType="help_sove.pojo.HelpSove">
        insert into help_sove(h_id,h_s_time,h_sovePerson,h_state,h_s_fa)
        values (#{h_id},#{h_s_time},#{h_sovePerson},#{h_state},#{h_s_fa})
    </insert>

</mapper>